{
  "name": "toxic-decorators",
  "version": "0.2.2",
  "description": "decorators",
  "main": "lib/toxic-decorators.js",
  "module": "lib/toxic-decorators.mjs",
  "jsnext:main": "lib/toxic-decorators.mjs",
  "browser": "lib/toxic-decorators.browser.js",
  "scripts": {
    "test": "jest --coverage && flow",
    "start": "jest --coverage --watch",
    "flow": "flow",
    "lint": "flow && eslint ./src --fix",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "build": "npm run b-common && npm run b-es && npm run b-umd && npm run b-min",
    "b-common": "rollup -c build/rollup.config.common.js",
    "b-es": "rollup -c build/rollup.config.es.js",
    "b-umd": "rollup -c build/rollup.config.umd.js",
    "b-min": "rollup -c build/rollup.config.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toxic-johann/toxic-decorators.git"
  },
  "keywords": [
    "decorators"
  ],
  "author": "toxic-johann",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/toxic-johann/toxic-decorators/issues"
  },
  "homepage": "https://github.com/toxic-johann/toxic-decorators#readme",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-jest": "^20.0.3",
    "flow-bin": "^0.49.1",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "nyc": "^11.0.3",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      ".*": "babel-jest"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "toxic-predicate-functions": "^0.1.2",
    "toxic-utils": "^0.1.1"
  }
}
